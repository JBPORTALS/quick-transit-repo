{
  "name": "@qt/wizard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@qt/ui": "workspace:^0.1.0",
    "geist": "^1.2.2",
    "lucide-react": "^0.363.0",
    "next": "^14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.2.25",
    "@qt/eslint-config": "workspace:^0.2.0",
    "@qt/prettier-config": "workspace:^0.1.0",
    "@qt/tailwind-config": "workspace:^0.1.0",
    "@qt/tsconfig": "workspace:^0.1.0",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/nextjs": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jiti": "^1.21.0",
    "prettier": "^3.2.5",
    "storybook": "^8.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@qt/eslint-config/base",
      "@qt/eslint-config/nextjs",
      "@qt/eslint-config/react"
    ]
  },
  "prettier": "@qt/prettier-config"
}
