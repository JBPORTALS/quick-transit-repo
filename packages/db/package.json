{
  "name": "@qt/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "push": "pnpm with-env drizzle-kit push:pg",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "seed": "pnpm with-env tsx  ./src/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@planetscale/database": "^1.16.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "postgres": "^3.4.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@qt/eslint-config": "workspace:^0.2.0",
    "@qt/prettier-config": "workspace:^0.1.0",
    "@qt/tsconfig": "workspace:^0.1.0",
    "@types/node": "^20.11.25",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.14",
    "esbuild": "^0.20.2",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@qt/eslint-config/base"
    ]
  },
  "prettier": "@qt/prettier-config"
}
